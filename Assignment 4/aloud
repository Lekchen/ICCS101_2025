def readAloud(lst: list[int])-> list[int]:
    result = list()
    i = 0
    while i < len(lst):
        current_num = lst[i]
        counts = 0
        while i < len(lst) and lst[i] == current_num:
            counts += 1
            i += 1
        result.append(counts)
        result.append(current_num)
    return result
